def logicoOr{
  var boolean op;
  var boolean opD;

  op = true;
  opD = true;

  if op || opD then
    println ("Verdadeiro");
  else
    println("Falso");
  endif
}

def logicoE{
    var boolean op;
    var boolean opD;

    op = true;
    opD = true;

    if op && opD then
      println ("Verdadeiro");
    else
      println("Falso");
    endif
}

def soma (int a, int b):int{

  var int res;

  res = a + b;

  return res;
}

def main{
  var int retorno;
  logicoOr();
  logicoE();
  retorno = soma(1,2,3);//mais parametros que o declarado
}
